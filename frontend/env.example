# API Configuration
# For local development:
REACT_APP_API_URL=http://localhost:8000

# For Firebase production (replace with your Cloud Run service URL):
# REACT_APP_API_URL=https://backend-XXXXX-uc.a.run.app

# Or use Firebase Hosting rewrites (no API_URL needed):
# REACT_APP_API_URL=/api

REACT_APP_API_TIMEOUT=10000

# Social Auth (Frontend)
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id
REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id
REACT_APP_TWITTER_CLIENT_ID=your-twitter-client-id

# Payment Processing (Frontend)
REACT_APP_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key
REACT_APP_PAYPAL_CLIENT_ID=your-paypal-client-id

# Environment
REACT_APP_ENVIRONMENT=development
